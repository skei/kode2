<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="kode2" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="debug.exe">
				<Option output="../../bin/kode_debug" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-DKODE_EXE" />
					<Add option="-DKODE_DEBUG" />
				</Compiler>
			</Target>
			<Target title="release.exe">
				<Option output="../../bin/kode_release" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-DKODE_EXE" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-std=c11" />
			<Add option="-m64" />
			<Add directory="../../src" />
		</Compiler>
		<Linker>
			<Add option="-m64" />
			<Add library="pthread" />
		</Linker>
		<Unit filename="../build.cpp" />
		<Extensions />
	</Project>
</CodeBlocks_project_file>
